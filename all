body.skin--responsive .mw-parser-output .infobox th:only-child {
  width: 100%;
}
body.skin--responsive .mw-parser-output .infobox tr:last-child th,
body.skin--responsive .mw-parser-output .infobox tr:last-child td {
  border: 0;
}
